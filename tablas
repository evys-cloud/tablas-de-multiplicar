#include <stdio.h>

int main(){
    int numero = 0;
    int i = 0;
    printf("* Tabla de Multiplicar *\n");

    printf("Ingrese un numero para mostrar su tabla de multiplicar:  ");
    scanf("%i", &numero);

    printf("\nTabla de Multiplicar del numero: %i\n", numero);


    for(i=0;i<=10;i++){
        printf("%i X %i = %i\n", numero, i, numero*i);

    }
    return 0;

}
